<% 
	var getIdMatchingVersion = function(version){ 
		for (var i=0; i < tab.length; i++) 
			if(tab[i].version == version) 
				return tab[i].id; 
		return "This version does not exist";
	} 
%>

Hello <%= title %>
v : <%= v %>
Id : <%= getIdMatchingVersion(v) %>

<script type="text/javascript">
	var tab = JSON.parse(<%= JSON.stringify(tab) %>);
</script>
