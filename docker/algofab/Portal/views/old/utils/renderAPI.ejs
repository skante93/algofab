		
		<% if(algo && verb && algo.API[verb]) { %>
            

			<div class="item">
				<div class="title" style="margin-bottom: 2em; margin-top: 2em; color: <%= (verb.toUpperCase() == 'GET')? 'green' : (verb.toUpperCase() == 'POST')? 'blue' : (verb.toUpperCase() == 'PUT')? 'grey' : 'red' %>">
					<%= verb.toUpperCase() %>
				</div>
				<div class="body">
					<% for (var i=0; i < algo.API[verb].length; i++) {%>
						<div class="row uri col-sm-offset-1 col-sm-11" id="empilement-<%= verb %>-<%= i+1 %>">
							<div class="item">
								<div class="col-sm-2 title"> <b> <%= algo.API[verb][i].uri %></b> </div>
								<div class="col-sm-10 body">

									<div class="row">
										<div class="col-sm-2"> Inputs : </div>
										<div class="col-sm-10">
											<table class="table">
									            <thead>
									                <tr>
														<th> Name </th>
														<th> Types </th>
														<th> Required </th>
													</tr>
												</thead>
												<tbody>
												<% for (var j=0; j < algo.API[verb][i].inputs.length; j++) { %>
													<tr>
			                    						<td> <%= algo.API[verb][i].inputs[j].name %> </td>
			                    						<td> <%= algo.API[verb][i].inputs[j].mime_types.join(', ') %> </td>
			                    						<td> <%= algo.API[verb][i].inputs[j].required %> </td>
			                    					</tr>
												<% } %>
												</tbody>
											</table>		
										</div>
									</div>

									<div class="row">
										<div class="col-sm-2"> Outputs : </div>
										<div class="col-sm-10"> 
											<% for (var j=0; j < algo.API[verb][i].outputs.length; j++) { %>
												<span> <%= algo.API[verb][i].outputs[j] %> </span>
											<% } %>
										</div>
									</div>

									<div class="row">
										<div class="col-sm-2"> Description : </div>
										<div class="col-sm-10"> 
											<%- algo.API[verb][i].description %>
										</div>
									</div>
								</div>
							</div>
						</div>
					<% } %>
				</div>
			</div>
      	<% } %>