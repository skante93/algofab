<% if(typeof iframing !== 'undefined') { %>
	<script type="text/javascript">
		window.top.window.addNewContent('<%= iframing %>');
	</script>
<% } else { %>
	<!DOCTYPE html>
	<html>
	  <head>
	    <% include partials/head %>
	    
	  </head>
	  <body>
	    <% include partials/header %>
	    <div class="container">
	    	<div id="done">

	    	</div>
	    </div>
	    <div class="container">
	    	<form method="post" target="myFrame">
	    		<div class="form-group row">
	    			<label class="col-sm-2">input : </label>
	    			<input type="text" class="col-sm-10" name="n_content"/>
	    		</div>
	    		<div class="form-group row">
	    			<input type="submit" class="pull-right" value="submit"/>
	    		</div>
	    	</form>
	    	<iframe src="#" id="myFrame" name="myFrame" style="display: none;"></iframe>
	    </div>
		
	    <% include partials/footer %>
	    <script type="text/javascript">
	    	function resultModal(algoname, algobody) {
	    		var mod = $('<div class="modal fade" role="dialog">'+
		                      '<div class="modal-dialog">'+

		                        '<!-- Modal content-->'+
		                        '<div class="modal-content">'+
		                          '<div class="modal-header">'+
		                            '<button type="button" class="close" data-dismiss="modal">&times;</button>'+
		                            '<h4 class="modal-title">'+algoname+'</h4>'+
		                          '</div>'+
		                          '<div class="modal-body">'+algobody+'</div>'+
		                          '<div class="modal-footer">'+
		                            '<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>'+
		                          '</div>'+
		                        '</div>'+

		                      '</div>'+
		                   '</div>');
		    	mod.modal('show');
	    	}
	    	function addNewContent(text) {
	    		if(typeof this.cmpt === 'undefined')
	    			this.cmpt = 0;
	    		$('#done').prepend($('<div class="row">'+
	    								'<div class="col-sm-2">input '+(this.cmpt)+' : </div>'+
	    								'<div class="col-sm-10">'+text+' : </div>'+
	    							'</div>'));
	    		resultModal('input '+this.cmpt, '<em>'+text+'</em>');
	    		
	    		this.cmpt++;
	    	}
	    	//resultModal('algonema', '<em><b>BODY</b></em>');
	    </script>
	  </body>
	</html>
<% } %>